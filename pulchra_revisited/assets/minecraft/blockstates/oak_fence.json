{
    "multipart": [
        {   "when": { "north": "true" },
            "apply": { "model": "block/oak_fence_north" }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "block/oak_fence_south" }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "block/oak_fence_south", "y": 90 }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "block/oak_fence_north", "y": 90 }
        },
        {   "when": { "north": "false", "south": "false", "west": "false", "east": "false" },
            "apply": { "model": "block/oak_fence_post" }
        },
        {   "when": { "north": "true", "south": "false", "west": "false", "east": "false" },
            "apply": { "model": "block/oak_fence_post"}
        },
        {   "when": { "north": "false", "south": "true", "west": "false", "east": "false" },
            "apply": { "model": "block/oak_fence_post"}
        },
        {   "when": { "north": "false", "south": "false", "west": "true", "east": "false" },
            "apply": { "model": "block/oak_fence_post"}
        },
        {   "when": { "north": "false", "south": "false", "west": "false", "east": "true" },
            "apply": { "model": "block/oak_fence_post"}
        },
        {   "when": { "north": "true", "west": "true" },
            "apply": { "model": "block/oak_fence_post"}
        },
        {   "when": { "north": "true", "east": "true" },
            "apply": { "model": "block/oak_fence_post"}
        },
        {   "when": { "south": "true", "west": "true" },
            "apply": { "model": "block/oak_fence_post"}
        },
        {   "when": { "south": "true", "east": "true" },
            "apply": { "model": "block/oak_fence_post"}
        }
    ]
}
