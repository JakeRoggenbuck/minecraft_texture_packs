{
    "multipart": [
        {   "when": { "up": "true" },
            "apply": { "model": "block/cobblestone_wall_post", "uvlock": false }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "block/cobblestone_wall_side", "uvlock": false }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "block/cobblestone_wall_side", "y": 90, "uvlock": false }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "block/cobblestone_wall_side", "y": 180, "uvlock": false }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "block/cobblestone_wall_side", "y": 270, "uvlock": false }
        }
    ]
}
